Abstract Contract ResolverBase(ansRegistryId: ByteVec) {
  enum PathPrefix {
    AddressInfo = #00
    NameInfo    = #01
    PubkeyInfo  = #02
  }

  pub fn getOwner(node: ByteVec) -> Address {
    let recordId = subContractIdOf!(ANSRegistry(ansRegistryId), node)
    let record = Record(recordId)
    return record.getOwner()
  }
}