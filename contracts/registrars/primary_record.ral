Contract PrimaryRecord(
  registrar: ByteVec,
  mut owner: Address,
  mut ttl: U256,
  refundAddress: Address,
  mut recordTokenId: ByteVec
) extends Constants(), RecordBase(registrar, owner, ttl, refundAddress) {
  @using(updateFields = true)
  pub fn setRecordTokenId(newRecordTokenId: ByteVec) -> () {
    checkCaller!(callerContractId!() == registrar, ErrorCodes.InvalidCaller)
    recordTokenId = newRecordTokenId
  }

  pub fn getRecordTokenId() -> ByteVec {
    return recordTokenId
  }
}